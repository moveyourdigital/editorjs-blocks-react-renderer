{
  "name": "editorjs-blocks-react-renderer",
  "version": "1.3.0",
  "description": "EditorJS blocks render to semanticly beautiful html5 tags via React.",
  "keywords": [
    "editorjs",
    "editor.js",
    "editor",
    "react",
    "render",
    "next.js",
    "nextjs",
    "gatsby",
    "renderer",
    "html5",
    "html",
    "blocks"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsc -w",
    "build": "tsc",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "format": "prettier --write \"src/**/*.tsx\"",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm run format && npm run lint && npm t && npm run build",
    "update:latest": "npm-check-updates -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moveyourdigital/editorjs-blocks-react-renderer.git"
  },
  "author": "Vitor Carvalho <lightningspirit@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moveyourdigital/editorjs-blocks-react-renderer/issues"
  },
  "homepage": "https://github.com/moveyourdigital/editorjs-blocks-react-renderer#README.md",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.4",
    "@types/react": "^17.0.48",
    "@types/react-dom": "^17.0.17",
    "@types/react-test-renderer": "^17.0.2",
    "jest": "^28.1.3",
    "npm-check-updates": "^16.0.5",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^28.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "html-react-parser": "^1.4.14"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "engines": {
    "node": ">=16"
  }
